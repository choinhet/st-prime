{
  "name": "st-prime",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:codegen": "concurrently \"cd .. && streamlit run app.py\" \"npm run dev\" \"wait-on http://localhost:5173 && wait-on http://localhost:8501 && playwright codegen http://localhost:8501\""
  },
  "dependencies": {
    "@primevue/core": "^4.1.1",
    "@primevue/themes": "^4.1.1",
    "@vueuse/core": "^11.1.0",
    "pinia": "^2.2.4",
    "primevue": "^4.1.1",
    "streamlit-component-lib": "^2.0.0",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@testing-library/vue": "^8.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.2.1",
    "vue-tsc": "^2.1.6",
    "wait-on": "^7.2.0"
  }
}